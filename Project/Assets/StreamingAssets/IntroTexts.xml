    <StringData sceneid="AA001_T" text=""/>
    <StringData sceneid="AA001_B" text=""/>
    <StringData sceneid="AA002_T" text="흔들흔들&#xA;가을바람이 좋아"/>
    <StringData sceneid="AA002_B" text="코스모스 위를 걸으면 나비가 날아가요"/>
    <StringData sceneid="AA003_T" text=""/>
    <StringData sceneid="AA003_B" text=""/>
    <StringData sceneid="AA004_T" text="반짝 반짝&#xA;반딧불이"/>
    <StringData sceneid="AA004_B" text="반딧불이와 함께 여름 분위기를 느껴볼까요?"/>
    <StringData sceneid="AA005_T" text="흔들흔들&#xA;가을바람이 좋아"/>
    <StringData sceneid="AA005_B" text="나비와 함께 가을 코스모스를 느껴보아요"/>
    <StringData sceneid="AA006_T" text=
    "&lt;color=#58CCFF&gt;뒤뚱뒤뚱&lt;/color&gt;
&lt;color=#58CCFF&gt;펭귄이 나타났어요!&lt;/color&gt;"/>
 
    <StringData sceneid="AA006_B" text="추운 남극의 펭귄을 보러 가볼까요?"/>
    <StringData sceneid="AA007_T" text="나무 속 작은세상&#xA;누가누가 살고있나요?"/>
    <StringData sceneid="AA007_B" text="게가 살고 있어요!&#xA;노크해볼까요?"/>
    <StringData sceneid="AA008_T" text="나무 속 작은세상&#xA;누가누가 살고있나요?"/>
    <StringData sceneid="AA008_B" text="나무 속 작은세상&#xA;누가누가 살고있나요?"/>
    <!-- 구분 -->
    <StringData sceneid="AB001_T" text="명화 미술놀이!"/>
    <StringData sceneid="AB001_B" text="밤하늘엔 무엇이 보일까?"/>
    <StringData sceneid="AB002_T" text="호랑이가 어디를 보고있을까?"/>
    <StringData sceneid="AB002_B" text="무시무시한 호랑이가 살아 움직이는 송하맹호도 속으로 들어가볼까요?"/>
    <!-- 구분 -->
    <StringData sceneid="BA001_T" text="터치터치!&#xA;그림을 원래대로 만들어요"/>
    <StringData sceneid="BA001_B" text="친구들과 함께 스케치를 밟아 호랑이에게 색을 돌려줘볼까요?"/>
    
    <StringData sceneid="BA002_T" text="터치터치!&#xA;그림을 원래대로 만들어요"/>
    <StringData sceneid="BA002_B" text="터치터치!&#xA;그림을 원래대로 만들어요"/>
    
    <StringData sceneid="BA005_T" text="오늘은 내가 요리사!&#xA;암냠냠 음식을만들어요!"/>
    <StringData sceneid="BA005_B" text="친구들과 함께 나만의 샌드위치를 만들어볼까요?"/>
    
    <StringData sceneid="BA006_T" text="손은 짝짝! 발은 쿵쿵!&#xA;그림도 그려볼까요?"/>
    <!-- -->
    <StringData sceneid="BA007_T" text="그림을 꾸며볼까?&#xA;명화 속 세상에 내가주인공!"/>
    <StringData sceneid="BA007_B" text="그림 위를 걸어 꾸밀 수 있어요"/>
    <!-- 구분 -->
    
    <StringData sceneid="BB001_T" text="친구와 신나는 공놀이 해볼까?"/>
    <StringData sceneid="BB002_T" text="한발 짝 두발 쿵!&#xA;순서대로 밟아요!"/>
    <StringData sceneid="BB002_B" text="친구와 같이 숫자 10까지 블록을 밟아보아요!"/>
    <StringData sceneid="BB003_T" text="알록달록&#xA;터치터치!"/>
    <StringData sceneid="BB003_B" text="동물친구들이 나타나기 전에 모든 발판을 뒤집어볼까요?"/>
    <StringData sceneid="BB004_T" text="알록달록&#xA;터치터치!"/>
    <StringData sceneid="BB004_B" text="누가 발판을 더 많이 뒤집는지 확인해볼까요?"/>
    <StringData sceneid="BB005_T" text="오션스타트!&#xA;쿵쾅쿵쾅!"/>
    <StringData sceneid="BB005_B" text="공을 구멍에 넣으면 돌고래가 나와 묘기를 부려요!"/>
    <StringData sceneid="BB006_T" text="오션스타트!&#xA;쿵쾅쿵쾅!"/>
    <StringData sceneid="BB006_B" text="공을 넣으면 돌고래가 나타나요"/>
    <StringData sceneid="BB007_T" text="애니멀 팡팡!&#xA;축구팡팡!"/>
    <StringData sceneid="BB007_B" text="공을 넣으면 돌고래가 나타나요"/>
 
    <StringData sceneid="BB008_T" text=
   "&lt;color=#FC6284&gt;요리조리 어디갔나?&lt;/color&gt;
&lt;color=#AFD485&gt;물고기 축제가 열렸어요!&lt;/color&gt;">
    </StringData>
    <StringData sceneid="BB008_B" text="바다속을 뛰어다니며 도망다니는 물고기를 잡아보아요!"/>
    <!-- 구분 -->
    <StringData sceneid="BC001_T" text="알록달록 낙엽을 밟으며&#xA;가을바람을 느껴보세요!"/>
    <StringData sceneid="BC001_B" text="낙엽 위를 걸어다니며 가을 낙엽 분위기를 느껴볼까요?"/>
    <StringData sceneid="BC002_T" text="호수에 사는 동물친구는&#xA;누가 있을까요?"/>
    <StringData sceneid="BC002_B" text="호수_Text_B"/>
    <!-- 구분 -->
    <StringData sceneid="BD001_T" text="딩동댕! &#xA;무지개 건반 "/>
    <StringData sceneid="BD001_B" text="하늘 가득 날아다니는 비눗방울을 터뜨리며 친구와 놀아요!"/>
    <StringData sceneid="BD004_T" text="랄랄라 통통통&#xA;악기놀이!"/>
    <StringData sceneid="BD004_B" text="열심히 뛰어다니며 앵무새를 신나게 춤추게 해볼까요?"/>
    <!-- 구분 -->

    <StringData sceneid="BB008_E_T"  text="&lt;color=#FC6284&gt;요리조리 어디갔나?&lt;/color&gt;
&lt;color=#AFD485&gt;물고기 축제가 열렸어요!&lt;/color&gt;"/>
    <StringData sceneid="BB008_E_B" text="바다속을 뛰어다니며 도망다니는 물고기를 잡아보아요!"/>

    <StringData sceneid="BB008_H_T"  text="&lt;color=#FC6284&gt;요리조리 어디갔나?&lt;/color&gt;
&lt;color=#AFD485&gt;물고기 축제가 열렸어요!&lt;/color&gt;"/>
    <StringData sceneid="BB008_H_B" text="바다속을 뛰어다니며 도망다니는 물고기를 잡아보아요!"/>
    
    <StringData sceneid="BB007_T" text="애니멀 팡팡!&#xA;축구 팡팡!"/>
    <StringData sceneid="BB008_B" text="바다속을 뛰어다니며 도망다니는 물고기를 잡아보아요!"/>
    <StringData sceneid="BB009_T" text="우주 팝잇!&#xA;캄캄한 우주 빙글빙글~"/>
    <StringData sceneid="BB009_B" text="여기저기 펼쳐져 있는 행성 친구들을 신나게 밟으며 재밌게 놀아볼까요?"/>
    <StringData sceneid="BB010_T" text="모양길 따라가기!"/>
    <StringData sceneid="BB011_T" text="손,발 트위스터&#xA;신나게 쿵쿵!"/>
    <StringData sceneid="BB012_T" text="까꿍! 문어야!&#xA;대왕문어가 나타났다!"/>
    <StringData sceneid="BB013_T" text="내가 좋아하는 것은 무엇인가요?"/>
    <StringData sceneid="BB013_B" text="내가 좋아하는 동물, 과일, 색깔을 골라보아요!"/>
    <!-- 구분 -->
   
    <StringData sceneid="BD002_T" text="딩동댕동!&#xA;무지개 건반!"/>
    <StringData sceneid="BD002_B" text="바다 위에 물고기 친구들과 음악놀이를 해보아요!"/>
    
    <StringData sceneid="BD003_T" text="통통통 통통통&#xA;비즈 드럼 연주!"/>
    <StringData sceneid="BD004_T" text="랄랄라~ 통통통!&#xA; 신나는 악기놀이!"/>
    <StringData sceneid="BD005_T" text="나처럼 해봐라 이렇게!"/>
    <StringData sceneid="BD006_T" text="쿵따쿵따 두드려 두드려!"/>

    <!-- 구분 -->
	<StringData sceneid="EA002_T" text="동물이 숲속에 놀러왔어요~!"/>
	<StringData sceneid="EA002_B" text="동물친구들과 숨바꼭질 놀이를 해요"/>
	<StringData sceneid="EA003_T" text="맛있는 과일과 야채가 있어요~"/>
	<StringData sceneid="EA003_B" text="알록달록 과일과 야채를 바구니에 담아볼까요?"/>
	<StringData sceneid="EA003_E_T" text="맛있는 과일과 야채가 있어요~"/>
	<StringData sceneid="EA003_E_B" text="알록달록 과일과 야채를 바구니에 담아볼까요?"/>

    <StringData sceneid="EA006_T" text="가을곡식을 지켜주는&#xA;허수아비 아저씨가 있어요~"/>
    <StringData sceneid="EA006_B" text="허수아비 아저씨를 찾아 도와줄까요?"/>
    
    <StringData sceneid="EA008_T" text="뽀드득 뽀드득!&#xA;목욕을해요~!"/>
    <StringData sceneid="EA008_B" text="몸을 깨끗하게 세균을 닦아요"/>
    <StringData sceneid="EA009_T" text="몸에 좋은 음식을 먹어요"/>
    <StringData sceneid="EA009_B" text="건강한 음식으로 바꿔줄까요?"/>

    <StringData sceneid="EA010_T" text="가을에는 주렁주렁 열매가 매달려요!&#xA; 어떤 열매가 있을까요?"/>
    <StringData sceneid="EA010_B" text="어떤 열매인지 알아 맞춰볼까요?"/>

